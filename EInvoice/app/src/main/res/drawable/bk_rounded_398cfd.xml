<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/color88398CFD" />
            <stroke android:width="@dimen/dp1" android:color="@color/color88398CFD" />
            <corners android:radius="@dimen/dp8" />
        </shape>
    </item>

    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/color398CFD" />
            <stroke android:width="@dimen/dp1" android:color="@color/color398CFD" />
            <corners android:radius="@dimen/dp8" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/color88398CFD" />
            <stroke android:width="@dimen/dp1" android:color="@color/color88398CFD" />
            <corners android:radius="@dimen/dp8" />
        </shape>
    </item>


</selector>